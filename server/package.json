    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "rimraf": "^3.0.2",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "typescript-rest": "^3.0.4"
  }
}
